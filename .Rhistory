get_wordcloud <- function(df1,colN) {
word<-na.omit(df1)
a<-data.frame(country = word$geo.name,
freq = (word[ , c(year)]^3))
par(pin = c(5.5,5.5))
wordcloud(a$country,a$freq,min.freq = 214599,scale=c(1,0.1),colors = a$freq)
}
